{
  "name": "work-sponsor",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run resize-images && remix build",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "resize-images": "rm -rf public/images/sizes && ts-node ./utils/resizeImages.ts",
    "setup:migrate": "prisma migrate dev",
    "setup:seed": "prisma db seed",
    "setup": "yarn setup:migrate && yarn setup:seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "typecheck": "tsc -b"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@remix-run/server-runtime": "^1.5.1",
    "@sendinblue/client": "^3.2.1",
    "framer-motion": "^6.3.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-intersection-observer": "^9.3.3",
    "react-loader-spinner": "^5.1.5",
    "react-remark": "^2.1.0",
    "react-transition-group": "^4.4.2",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "sharp": "^0.30.6",
    "sib-api-v3-sdk": "^8.3.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@ngneat/falso": "^5.6.0",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-transition-group": "^4.4.4",
    "@types/sharp": "^0.30.4",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "cypress": "^9.6.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^3.2.2",
    "msw": "^0.40.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "prisma": "^3.14.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.8"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
